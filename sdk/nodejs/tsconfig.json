        "config/index.ts",
        "config/vars.ts",
        "container.ts",
        "docker.ts",
        "getNetwork.ts",
        "getRegistryImage.ts",
        "image.ts",
        "index.ts",
        "network.ts",
        "provider.ts",
        "remoteImage.ts",
        "secret.ts",
        "service.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts",
        "volume.ts"
    ]
}

